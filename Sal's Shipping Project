weight = 41.5
#try with other weights!

#ground shipping
cost_ground_premium = 125.00
if weight <= 2.0:
  cost_ground = weight * 1.50 + 20.00
elif weight <= 6.0:
  cost_ground = weight * 3.00 + 20.00
elif weight <= 10.00:
  cost_ground = weight * 4.00 + 20.00
else:
  cost_ground = weight * 4.75 + 20.00
print("Ground Shipping $", cost_ground)
print("Ground Shipping Premium $", cost_ground_premium)

#drone shipping
if weight <= 2.0:
  cost_drone = weight * 4.50
elif weight <= 6.0:
  cost_drone = weight * 9.00
elif weight <= 10.0:
  cost_drone = weight * 12.00
else:
  cost_drone = weight * 14.25
print("Drone Shipping $", cost_drone)

#cheapest cost
cheapest = min(cost_ground, cost_ground_premium, cost_drone)
if cheapest == cost_ground:
  print("The cheapest option is Ground Shipping: $", cost_ground)
elif cheapest == cost_ground_premium:
  print("The cheapest option is Ground Shipping Premium: $", cost_ground_premium)
else:
  print("The cheapest option is Drone Shipping: $", cost_drone)
